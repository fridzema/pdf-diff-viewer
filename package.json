{
  "name": "pdf-diff",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && npm run copy-worker",
    "copy-worker": "cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/pdf.worker.min.mjs",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{js,ts,vue,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,ts,vue,json,md}\"",
    "typecheck": "nuxt typecheck",
    "prepare": "husky || true"
  },
  "dependencies": {
    "@nuxt/ui": "^2.18.4",
    "diff2html": "^3.4.52",
    "nuxt": "^3.13.0",
    "pdfjs-dist": "^4.7.76",
    "vue": "^3.5.24",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@nuxt/eslint-config": "^1.10.0",
    "@nuxtjs/tailwindcss": "^6.12.1",
    "@types/node": "^20.16.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/coverage-v8": "^4.0.8",
    "@vitest/ui": "^4.0.8",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "^5.6.0",
    "typescript-eslint": "^8.46.4",
    "vitest": "^4.0.8"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
